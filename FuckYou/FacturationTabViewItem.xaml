            <TextBox x:Name="PatientLienNeNumber" HorizontalAlignment="Left" Height="28" Margin="476,127,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="18"/>
            <ComboBox x:Name="PatientCodeRemb" Margin="146,193,0,0" Width="304" Height="28"/>
            <ComboBox x:Name="PatientMutuCB" Margin="146,226,0,0" Width="214" Height="28"/>
            <ComboBox x:Name="PatientContral" Margin="146,259,0,0" Width="454" Height="28"/>
            <CheckBox x:Name="PatientMutu" Content="Mutu" Margin="22,222,0,0" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="22,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Text="Nom" FocusVisualPrimaryBrush="#E4000000"/>
            <TextBlock HorizontalAlignment="Left" Margin="22,99,0,0" Text="Prenom" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="22,132,0,0" Text="Lien/Ne(e)" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="22,264,0,0" Text="Contral" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="22,165,0,0" Text="TeVe-mail" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="22,297,0,0" Text="N* Adh Mutu" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="22,330,0,0" Text="Droits" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="22,198,0,0" Text="Code Remb" TextWrapping="Wrap" VerticalAlignment="Top"/>
        </Grid>
        <Grid x:Name="PrescripteurTab" Grid.Column="2" HorizontalAlignment="Left" Height="418" Margin="242,3,0,0" VerticalAlignment="Top" Width="610" Grid.RowSpan="2">
            <Rectangle x:Name="PrescripteurRect" HorizontalAlignment="Center" Height="418" Stroke="Black" VerticalAlignment="Center" Width="610" RadiusX="10" RadiusY="10" Fill="#FFC9C8C8"/>
            <TextBlock HorizontalAlignment="Left" Margin="24,22,0,0" Text="Prescripteur" TextWrapping="Wrap" VerticalAlignment="Top" Height="36" Width="550" Foreground="#FF652BB9" FontSize="25"/>
            <TextBox x:Name="PrescripteurNom" HorizontalAlignment="Left" Height="28" Margin="146,61,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="428"/>
            <TextBox x:Name="PrescripteurPrenom" HorizontalAlignment="Left" Height="28" Margin="146,94,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="428"/>
            <TextBox x:Name="PrescripteurRPPS" HorizontalAlignment="Left" Height="28" Margin="146,160,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="428"/>
            <TextBox x:Name="PrescripteurIdFiness" HorizontalAlignment="Left" Height="28" Margin="146,193,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="428"/>
            <TextBox x:Name="PrescripteurTel" HorizontalAlignment="Center" Height="28" Margin="0,259,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="319"/>
            <ComboBox x:Name="PrescripteurHopital" Margin="146,127,0,0" Width="428" Height="28"/>
            <ComboBox x:Name="PrescripteurSpec" HorizontalAlignment="Center" Margin="0,226,0,0" Width="319" Height="28"/>
            <ComboBox x:Name="PrescripteurSpecType" Margin="470,259,0,0" Width="104" Height="28"/>
