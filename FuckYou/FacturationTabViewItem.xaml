            <TextBlock HorizontalAlignment="Left" Margin="24,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="36" Height="18" RequestedTheme="Light"><Run Text="Nom"/><LineBreak/><Run/></TextBlock>
            <TextBlock HorizontalAlignment="Left" Margin="24,99,0,0" Text="Prenom" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="24,132,0,0" Text="Hopital" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="24,165,0,0" Text="RPPS" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="24,198,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Text="ID/Finess"/>
            <TextBlock HorizontalAlignment="Left" Margin="24,264,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Text="Tel"/>
            <TextBlock HorizontalAlignment="Left" Margin="24,371,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Text="Date fac"/>
            <TextBlock HorizontalAlignment="Left" Margin="24,338,0,0" Text="Date/orig ord" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <ComboBox x:Name="PrescripteurDateOrig" HorizontalAlignment="Center" Margin="0,333,0,0" Width="319" Height="28"/>
            <ComboBox x:Name="PrescripteurDateOrigOrd" Margin="470,333,0,0" Width="104" Height="28"/>
            <ComboBox x:Name="PrescripteurDateFac" Margin="146,366,0,0" Width="428" Height="28"/>
            <TextBlock HorizontalAlignment="Left" Margin="24,231,0,0" Text="Spec" TextWrapping="Wrap" VerticalAlignment="Top"/>
